{
  "name": "app_builder",
  "version": "1.0.0",
  "description": "The app builder project",
  "main": "module.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "make test",
    "postinstall": "node setup/setup.js",
    "postupdate": "node setup/setup.js",
    "build": "node_modules/.bin/webpack",
    "watch": "node_modules/.bin/webpack --watch --progress",
    "testbuild": "node_modules/.bin/webpack --config ./assets/opstools/AppBuilder/test/webpack.config.js",
    "testwatch": "node_modules/.bin/webpack --config ./assets/opstools/AppBuilder/test/webpack.config.js --watch --progress",
    "testclient": "node_modules/mocha-phantomjs/bin/mocha-phantomjs -p node_modules/phantomjs/bin/phantomjs assets/opstools/AppBuilder/test/test-all.html --reporter dot --ignore-resource-errors ",
    "testcover": "node_modules/nyc/bin/nyc.js node_modules/mocha/bin/mocha -p node_modules/phantomjs/bin/phantomjs assets/opstools/AppBuilder/test/test-all.html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/appdevdesigns/app_builder.git"
  },
