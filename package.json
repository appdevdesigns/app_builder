    "ad-utils": "appdevdesigns/ad-utils",
    "appdev-cli": "appdevdesigns/appdev-cli",
    "async": "^1.5.2",
    "knex": "^0.13.0",
    "lodash": "^4.13.1",
    "moment": "^2.18.1",
    "mysql": "^2.13.0",
    "node-uuid": "^1.4.8",
