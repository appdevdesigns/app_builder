    "uuid": "^3.1.0",
    "waterline-sqlite3": "^50.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "barrels": "^1.6.4",
    "chai": "*",
    "css-loader": "^0.28.1",
    "es6-promise": "^4.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
