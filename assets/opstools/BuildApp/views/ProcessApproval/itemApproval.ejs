<div>
	<% if (title && title.header) { %>
		<div class="page-header">
			<h1>
				<%= title.header %>
			</h1>
		</div>
		<% } %>
			<% if (headerInfo) { %>
				<ul>
					<% Object.keys(headerInfo.attr()).forEach(function(key) { %>
						<li>
							<%= key %> :
								<%= headerInfo[key] %>
						</li>
						<%  }); %>
				</ul>
				<% } %>
</div>
<div>

	<div class="panel panel-info">
		<% if (title && title.detail) { %>
			<div class="panel-heading">
				<b><%= title.detail %></b>
			</div>
			<% } %>
				<div class="panel-body">
					<ul class="list-group row" style="-webkit-box-shadow: 0 0px; box-shadow: 0 0px;">
						<% Object.keys(data.attr()).forEach(function(key) { %>
							<% if (key == 'id') return; %>
								<li class="list-group-item col-xs-6" style="border: 0 none;">
									<b><%= key %></b> :
									<%= data[key] %>
								</li>
								<% }); %>
					</ul>
				</div>
	</div>

</div>