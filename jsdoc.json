{
   "recurseDepth": 15,
   "source": {
      "include": ["./api/"],
      "exclude": ["./api/classes/platform/dataFields/[template].js"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
   },
   "sourceType": "module",
   "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
   },
   "opts": {
      "template": "./node_modules/jsd-jekyll",
      "destination": "./docs/",
      "recurse": true,
      "repos": [
         {
            "folder": "app_builder",
            "name": "appdevdesigns/app_builder"
         },
         {
            "folder": "core",
            "name": "appdevdesigns/appbuilder_class_core"
         }
      ],
      "includeAll": true,
      "functionFix": true,
      "folderCategory": true
   },
   "plugins": ["plugins/markdown", "./node_modules/jsd-jekyll"]
}
